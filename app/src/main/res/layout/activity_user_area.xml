    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="34dp"
        android:textSize="50dp"
        android:textStyle="bold"
        android:textColor="@color/colorAccent"
        android:text="My Area"
        android:layout_below="@+id/linearLayout"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/bPlan"
        android:layout_width="100dp"
        android:layout_height="150dp"
        android:layout_marginLeft="54dp"
        android:layout_marginStart="54dp"
        app:srcCompat="@drawable/myplanimage"
        android:layout_marginBottom="18dp"
        android:layout_above="@+id/bProfil"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:id="@+id/bSearch"
        android:layout_width="100dp"
        android:layout_height="150dp"
        android:layout_marginEnd="55dp"
        android:layout_marginRight="55dp"
        app:srcCompat="@drawable/searchimage"
        android:layout_alignTop="@+id/bPlan"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:id="@+id/bMessage"
        android:layout_width="100dp"
        android:layout_height="150dp"
        android:layout_marginBottom="52dp"
        app:srcCompat="@drawable/messageimage"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/bPlan"
        android:layout_alignStart="@+id/bPlan" />

    <ImageView
        android:id="@+id/bProfil"
        android:layout_width="100dp"
        android:layout_height="150dp"
        app:srcCompat="@drawable/profileimage"
        android:layout_alignTop="@+id/bMessage"
        android:layout_alignLeft="@+id/bSearch"
        android:layout_alignStart="@+id/bSearch" />


